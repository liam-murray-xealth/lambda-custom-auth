{
  "name": "lambda",
  "version": "0.1.0",
  "description": "Authorizor for OAuth",
  "main": "./dist/index.js",
  "private": true,
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "lambda": "rimraf lambda && cpx 'dist/src/**/*.js' lambda && cpx 'package*.json' lambda && cd lambda && npm ci --production",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "env TS_NODE_COMPILER=ttypescript mocha test",
    "test:cov": "env TS_NODE_COMPILER=ttypescript nyc npm test",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "markdownlint .",
    "lint:eslint": "eslint --cache --ext .ts ./",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/aws-lambda": "^8.10.51",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/eslint": "^6.8.0",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/jwk-to-pem": "^2.0.0",
    "@types/lodash.get": "^4.4.6",
    "@types/markdownlint": "^0.13.0",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.5",
    "@types/pino": "^6.0.0",
    "@types/prettier": "^2.0.0",
    "@types/sinon": "^9.0.0",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "env-var": "^6.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "jsonwebtoken": "^8.5.1",
    "jwk-to-pem": "^2.0.3",
    "lodash.get": "^4.4.2",
    "markdownlint": "^0.20.2",
    "markdownlint-cli": "^0.23.0",
    "mocha": "^7.1.2",
    "npm-audit-resolver": "^2.2.0",
    "pino": "^6.2.1",
    "prettier": "^2.0.5",
    "sinon": "^9.0.2",
    "sinon-chai": "^3.5.0",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "typescript-transform-paths": "^1.1.14"
  },
  "repository": {
    "type": "git",
    "url": "na"
  },
  "author": "Nobody",
  "license": "",
  "readme": "./README.md"
}
