{
  "name": "lambda",
  "version": "1.0.0",
  "description": "custom auth lambda",
  "main": "./index.js",
  "private": true,
  "scripts": {
    "audit-checker": "check-audit",
    "audit-resolver": "resolve-audit",
    "build": "npm run build:clean && npm run build:dist",
    "build:clean": "rimraf dist",
    "build:dist": "tsc",
    "lambda": "rimraf lambda && cpx 'dist/src/**/*.js' lambda && cpx 'package*.json' lambda && cd lambda && npm ci --production",
    "format": "npm run format:prettier",
    "format:prettier": "prettier --write '**/*.{ts,js,md,json}'",
    "test": "env TS_NODE_COMPILER=ttypescript mocha test",
    "test:cov": "env TS_NODE_COMPILER=ttypescript nyc npm test",
    "lint": "npm run lint:mdlint && npm run lint:eslint",
    "lint:mdlint": "markdownlint .",
    "lint:eslint": "eslint --cache --ext .ts ./",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch"
  },
  "dependencies": {
    "env-var": "^6.1.1",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/aws-lambda": "^8.10.51",
    "@types/lodash.get": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.13.5",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "aws-sdk": "^2.672.0",
    "aws-sdk-mock": "*",
    "babel-plugin-module-resolver": "^4.0.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "cpx": "^1.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-mocha": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "markdownlint-cli": "^0.23.0",
    "mocha": "^10.2.0",
    "npm-audit-resolver": "^2.2.0",
    "prettier": "^2.0.5",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "ttypescript": "^1.5.10",
    "typescript": "^3.8.3",
    "typescript-transform-paths": "^1.1.14"
  },
  "repository": {
    "type": "git",
    "url": "na"
  },
  "author": "",
  "license": "MIT",
  "readme": "../README.md"
}
